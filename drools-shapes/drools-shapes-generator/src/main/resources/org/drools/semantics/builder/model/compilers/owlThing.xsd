            xmlns:hj="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations"
            jaxb:extensionBindingPrefixes="hj"
            elementFormDefault="qualified"
            targetNamespace="http://www.w3.org/2002/07/owl">

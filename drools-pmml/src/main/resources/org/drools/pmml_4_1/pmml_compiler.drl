    $svm  : SupportVectorMachine( this memberOf $info,
                                  $bias : coefficients.absoluteValue,
                                  $noc : coefficients.numberOfCoefficients,
                                  $nos : supportVectors.numberOfSupportVectors,
                                  $coeffs : coefficients, $svs : supportVectors,

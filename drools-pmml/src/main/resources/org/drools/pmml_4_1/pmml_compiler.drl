query formatArray( Array $arr, String $out )
    $arr := Array( $type : type, $content : content )
    $out := String() from accumulate (
        $s : String() from utils.tokenize( $content ),
        init( String x = ""; ),
        action( x = ( "".equals(x) ? ", " : "" ) + utils.format( $type, $s ) + x; ),
        result( x )
    )
end







